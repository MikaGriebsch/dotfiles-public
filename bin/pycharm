#!/bin/bash
#Generated by JetBrains Toolbox 2.6.0.40632 at 2025-04-20T21:45:55

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/andy/Applications/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${intellij_args[@]}"