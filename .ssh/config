Include config-private

# make sure to reference a key that can access your private repo
# otherwise the installation will fail once any other IdentityFile
# is referenced that might not be present on the new machine
IdentityFile ~/.ssh/id_rsa

Host *
  PasswordAuthentication no
  KbdInteractiveAuthentication no
  ChallengeResponseAuthentication no
  ExitOnForwardFailure yes
  ServerAliveInterval 60
  ConnectTimeout 10
  AddKeysToAgent yes
  EscapeChar `
  ControlMaster auto
  ControlPersist 72000
  ControlPath ~/.ssh/s/%C
